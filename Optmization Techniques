   "outputs": [],
   "source": [
    "Ackley <- function(x,d){\n",
    "    \n",
    "    return (-20*exp(-0.2*sqrt(sum(x^2)/d)) -exp( sum(cos(2*pi*x))/d) + 20 +exp(1))\n",
    "    \n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "7c9169b4-27d9-4a5e-acbc-fb06656dec3c",
   "metadata": {},
   "outputs": [],
   "source": [
    "ackley <- function(x) {\n",
    "  d <- length(x)\n",
    "  sum1 <- sum(x^2)\n",
    "  sum2 <- sum(cos(2 * pi * x))\n",
    "  term1 <- -20 * exp(-0.2 * sqrt(1/d * sum1))\n",
    "  term2 <- -exp(1/d * sum2)\n",
    "  f <- term1 + term2 + 20 + exp(1)\n",
    "  return(f)\n",
    "}"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "ecd5ba10-cf33-4245-8274-ab49a21d4db8",
   "metadata": {},
